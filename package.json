{
  "name": "lunii",
  "version": "1.0.0",
  "description": "Modern Electron Discord Self-Bot Dashboard",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder"
  },
  "keywords": [
    "electron",
    "discord",
    "selfbot",
    "dashboard"
  ],
  "author": "Lunii Team",
  "license": "MIT",
  "devDependencies": {
    "electron": "^26.0.0",
    "electron-builder": "^24.6.4"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.11.0",
    "discord.js-selfbot-v13": "^2.15.1",
    "node-cron": "^4.2.1"
  },
  "build": {
    "appId": "com.lunii.dashboard",
    "productName": "Lunii",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "assets/**/*"
    ]
  }
}
